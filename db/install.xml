<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/kent/db" VERSION="20140430" COMMENT="XMLDB file for Moodle local/kent"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="local_kent_trackers" COMMENT="Tracking table for tracking various statistics">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="value" TYPE="int" LENGTH="11" NOTNULL="false" DEFAULT="0" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="k_name" TYPE="unique" FIELDS="name"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>